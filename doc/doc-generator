import os, sys
import argparse
from datetime import date

def executeRakeFile():
    bundlerGenerator = 'bundle exec rake build'
    os.system(bundlerGenerator)

if __name__ == "__main__":
    if (os.path.exists('Rakefile')):
        executeRakeFile()
    else:
        pass
